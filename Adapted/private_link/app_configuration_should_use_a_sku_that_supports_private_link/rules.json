{
    "if": {
        "allOf": [
         {
          "field": "type",
          "equals": "Microsoft.AppConfiguration/configurationStores"
         },
         {
          "field": "Microsoft.AppConfiguration/configurationStores/sku.name",
          "equals": "Free"
         }
        ]
       },
    "then": {
        "effect": "[parameters('effect')]"
    }
}